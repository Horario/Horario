<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_qrScanner_relativeLayout_final_decission_main"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".TabActivity">

    <TextView
        android:id="@+id/dialog_qrScanner_textView_question"
        android:layout_width="match_parent"
        android:layout_height="216dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="78dp" />

    <Button
        android:id="@+id/dialog_event_final_decission_accept"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="126dp"
        android:background="@color/zentea_darkgreen"
        android:text="@string/accept"
        android:textColor="@color/zentea_white"
        android:visibility="visible"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="119dp" />

    <Button
        android:id="@+id/dialog_event_final_decission_reject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/dialog_event_final_decission_accept"
        android:layout_marginEnd="108dp"
        android:background="@color/zentea_darkgreen"
        android:text="@string/reject"
        android:textColor="@android:color/background_light"
        android:visibility="visible"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="7dp" />

</RelativeLayout>
