<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_qrScanner_relativeLayout_question_main"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".TabActivity">


    <RelativeLayout
        android:id="@+id/dialog_qrScanner_relativeLayout_QuestionFrame"
        android:layout_width="382dp"
        android:layout_height="290dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <TextView
            android:id="@+id/dialog_qrScanner_textView_question"
            android:layout_width="match_parent"
            android:layout_height="216dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="78dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/dialog_qrScanner_relativLayout_buttonFrame_finalQuestion"
        android:layout_width="match_parent"
        android:layout_height="240dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="301dp">

        <Button
            android:id="@+id/dialog_qrScanner_button_accept"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/dialog_qrScanner_button_eventSave"
            android:layout_marginStart="84dp"
            android:text="@string/accept"
            android:visibility="visible"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="119dp" />

        <Button
            android:id="@+id/dialog_qrScanner_button_eventSave"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="77dp"
            android:layout_marginTop="64dp"
            android:text="@string/reject"
            android:visibility="visible"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="7dp" />

    </RelativeLayout>


</RelativeLayout>